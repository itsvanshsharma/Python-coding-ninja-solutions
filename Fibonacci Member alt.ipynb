    "def checkFib(n):\n",
    "    if n==1 or n==0:\n",
    "        return True\n",
    "    a = 1\n",
    "    b = 1\n",
    "    while a<n:\n",
    "        a, b = a+b, a\n",
    "    if a==n:\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "n = int(input())\n",
    "if checkFib(n):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
  }
