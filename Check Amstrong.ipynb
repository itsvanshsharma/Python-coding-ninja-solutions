   "source": [
    "num = int(input())\n",
    
    "copy = num\n",
    "n=0\n",
    "while(copy>0):\n",
    "    n=n+1  # counting number of digits\n",
    "    copy=copy//10\n",
    "\n",
    
    "sum = 0\n",
    "temp = num\n",
    "while temp > 0:\n",
    "   digit = temp % 10\n",
    "   sum += digit ** n #number of digits in power\n",
    "   temp //= 10\n",
    "if num == sum:\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
  }
